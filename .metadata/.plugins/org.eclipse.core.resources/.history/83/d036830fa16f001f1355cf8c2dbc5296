package com.lucas.demo.service;

import javax.xml.bind.JAXBException;

import com.lucas.demo.model.Pedido;

public class Main {
    public static void main(String[] args) {
    	 String xml = "<pedido><id>123</id><quantity>2</quantity><description>Test product</description></pedido>";
        
        PedidoServico pedidoService = new PedidoServico();
        
        try {
            Pedido pedido = pedidoService.xmlParaPedido(xml);
            
            // Exibir os dados do objeto Pedido
           // System.out.println("ID: " + pedido.getId());
            System.out.println("Quantidade: " + pedido.getQuantity());
            //System.out.println("Descrição: " + pedido.getDescription());
            
        } catch (JAXBException e) {
            e.printStackTrace();
        }
    }
}